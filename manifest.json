{
  "manifest_version": 2,
  "name": "IXmaps reporter",
  "description": "Connects to IXmaps",
  "icons": {
      "128": "ixmaps-icon.png"
  },
  "version": "1.0.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ajax.googleapis.com; object-src 'self'",
  "permissions": [
      "http://*/",
      "history", "storage", "geolocation", "webRequest", "history"
  ],
  "app": {
    "launch": {
      "local_path": "src/app.html"
    }
  }
}
